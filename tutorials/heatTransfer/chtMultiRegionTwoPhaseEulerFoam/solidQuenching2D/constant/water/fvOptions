FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitTLiq
{
    type       limitTemperature;
    min        300;
    max        2000;
    selectionMode all;
    phase       liquid;
    active      yes;
}

limitTGas
{
    type       limitTemperature;
    min        300;
    max        2000;
    selectionMode all;
    phase       gas;
    active      yes;
}

