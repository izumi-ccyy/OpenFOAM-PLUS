FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.1 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(solarpanel.*|ref_wall|YMax|YMin|XMax|XMin|ZMin)"
    {
        type            fixedValue;
        value           $internalField;
    }

    ZMax
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
}


